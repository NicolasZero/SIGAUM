{
  "name": "multi_package_project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "api:start": "cd api && npm run start",
    "api:dev": "cd api && npm run dev",
    "api:install": "cd api && npm install",
    "web:start": "cd web && pnpm start",
    "web:dev": "cd web && pnpm dev",
    "web:install": "cd web && pnpm install",
    "dev": "npm-run-all --parallel api:dev web:dev",
    "start": "npm-run-all --parallel api:start web:start",
    "all": "npm-run-all --parallel api:install web:install"
  },
  "keywords": [],
  "author": "Nicolas J.S Zapata M.",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
